<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/head') %>    
    <body class="kiosco-body">

        <% if (typeof user === 'undefined') { %>
            <%- include('./partials/header') %>
        <% } else { %>
            <%- include('./partials/headerLoggedIn') %>
        <% } %>

        <main>
            <h1>Todas las películas</h1>
            <ul>
                <% for (let i= 0; i<productos.length; i++) { %>
                    <li> <img src="<%= productos[i].poster %>" alt="póster de la película"> <%= productos[i].titulo %></li>
                <% } %>
            </ul>
        </main>

        <%- include('./partials/footer') %>
        
    </body>
</html>